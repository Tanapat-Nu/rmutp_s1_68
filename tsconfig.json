{
  "compilerOptions": {
    "target": "ES2022",
    "declaration": true,
    "moduleResolution": "Bundler",
    "outDir": "dist",
    "rootDir": "app",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "types": [
      "node",
      "vitest/globals"
    ]
  },
  "include": [
    "app"
  ],
  "exclude": [
    "node_modules",
    
  ]
}
